      "cell_type": "code",
      "source": [
        "import requests\n",
        "url = \"https://simple.wikipedia.org/wiki/Global_warming\"\n",
        "headers = {\n",
        "    \"User-Agent\": \"Mozilla/5.0\"\n",
        "}\n",
        "response = requests.get(url, headers=headers)\n",
        "source = response.text\n",
        "print(source)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
